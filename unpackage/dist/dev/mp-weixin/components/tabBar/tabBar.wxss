
.tab-bar {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  border-top: 1px solid #ddd;
}
.tab-item {
  flex: 1;
  text-align: center;
  padding: 10rpx 0;
}
.tab-item image {
  width: 50rpx;
  height: 50rpx;
}
.tab-item text {
  display: block;
  font-size: 24rpx;
  color: #666;
}
.tab-item text.active {
  color: #007AFF;
}
